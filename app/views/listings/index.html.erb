<div class="center">
  <div class="row">
    <% @listings.each do |listing| %>
    <div class="col-md-4">
      <div class="thumbnail">
        <%= image_tag listing.image.url %>
        <div class="caption">
          <h3><%= listing.name %></h3>
          <p><%= number_to_currency(listing.price) %></p>
          <p><%= "Sold by #{listing.user.name}"%></p>
          <%= link_to 'Show', listing, class: "btn btn-default" %>
          <%= link_to 'Edit', edit_listing_path(listing), class: "btn btn-primary" %>
          <%= link_to 'Destroy', listing,class: "btn btn-danger", method: :delete, data: { confirm: 'Are you sure?' } %>
        </div>
      </div>
    </div>
    <%end%>
  </div>



  <%= link_to 'New Listing', new_listing_path, class: "btn btn-success" %>
</div>  